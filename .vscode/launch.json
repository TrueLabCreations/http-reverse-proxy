{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Test JS",
      "program": "${workspaceFolder}/src/testJS.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Tests",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "-u",
        "tdd",
        "--timeout",
        "120000",
        "--colors",
        "${workspaceFolder}/dist/test/runInternalTests.js"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "cwd": "${workspaceFolder}/dist"
    },
    {
      "type": "node",
      "request": "launch",
      "name":"Run Internal Test",
      "env":
        {
          "NODE_ENV":"development"
        },
      "program": "${workspaceFolder}/test/runInternalTests.ts",
      "outFiles": [
        "${workspaceFolder}/dist/test/*.js"
      ],
      "cwd": "${workspaceFolder}/dist"
    },
    {
      "type": "node",
      "request": "launch",
      "name":"Run External Test",
      "env":
        {
          "NODE_ENV":"development"
        },
      "program": "${workspaceFolder}/test/runExternalTests.ts",
      "outFiles": [
        "${workspaceFolder}/dist/test/*.js"
      ],
      "cwd": "${workspaceFolder}/dist"
    },
    {
      "type": "node",
      "request": "launch",
      "name":"Run Server",
      "env":
        {
          "NODE_ENV":"development"
        },
      "program": "${workspaceFolder}/src/index.ts",
      "outFiles": [
        "${workspaceFolder}/dist/src/*.js"
      ],
      "cwd": "${workspaceFolder}/dist"
    }
  ]
}